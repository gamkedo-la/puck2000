[sub_resource type="GDScript" id=2]
resource_name = "add_puck_select_btns"
script/source = "extends VBoxContainer

# this script will instantiate the number of buttons based on the number of pucks in $\"../ViewportContainer/Viewport/Pucks\"

export (PackedScene) var button

onready var pucks = $\"../ViewportContainer/Viewport/Pucks\"


func _ready() -> void:
	# use children nodes in pucks to set number of buttons
	var puck_items = pucks.get_children()
	var puck_items_size = puck_items.size()
	for i in range(puck_items_size):
		var button_instance = button.instance()
		add_child(button_instance)
	
	# after buttons have been instantiated, run their methods to set up the buttons
	for n in range(puck_items_size):
		var cur_btn = get_child(n)
		cur_btn.set_focus()
		cur_btn.set_button_neighbours()
		var puck_name = puck_items[n].name
		cur_btn.set_text(str(puck_name))
"

